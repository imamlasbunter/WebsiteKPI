<script>
    function downloadAndReload(event, documentName) {
        event.preventDefault(); // Mencegah tindakan bawaan tautan
        var downloadUrl = event.target.href; // URL unduhan

        // Membuat permintaan unduhan menggunakan AJAX
        var xhr = new XMLHttpRequest();
        xhr.open("GET", downloadUrl, true);
        xhr.responseType = "blob";

        xhr.onload = function () {
            if (xhr.status === 200) {
                var blob = xhr.response;
                var link = document.createElement("a");
                link.href = window.URL.createObjectURL(blob);
                link.download = documentName + ".doc";
                link.click();

                // Memuat ulang halaman setelah unduhan selesai
                location.reload();
            }
        };

        xhr.send();
    }
</script>


